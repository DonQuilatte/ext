{
  "name": "ishka-extension",
  "version": "3.9.7",
  "description": "The Ultimate ChatGPT Experience! Prompt Library, Export, Pin, Subfolders, Search, RTL, Save Prompts, Download as MP3 & more!",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "dev": "webpack --mode=development --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.{ts,js,css,json}",
    "test": "jest",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:functional": "jest --testPathPattern=functional",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:all": "npm run test:smoke && npm run test:functional && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:extension": "jest tests/extension-lifecycle-test.js --verbose --testPathPattern=extension-lifecycle-test",
    "test:extension:ci": "jest tests/extension-lifecycle-test.js --verbose --ci --testPathPattern=extension-lifecycle-test",
    "test:extension:headless": "HEADLESS=true jest tests/extension-lifecycle-test.js --verbose --testPathPattern=extension-lifecycle-test",
    "setup": "mkdir -p test-results && mkdir -p screenshots",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "report": "open test-results/bug-report.html"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/chrome": "^0.0.246",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.55.0",
    "fs-extra": "^11.3.0",
    "glob": "^10.4.5",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.0.1",
    "mini-css-extract-plugin": "^2.7.0",
    "prettier": "^3.1.0",
    "puppeteer": "^22.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "testTimeout": 120000,
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js",
      "<rootDir>/tests/extension-lifecycle-test.js"
    ],
    "verbose": true,
    "testEnvironment": "jest-environment-puppeteer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DonQuilatte/ext.git"
  },
  "keywords": [
    "chrome-extension",
    "chatgpt",
    "typescript",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DonQuilatte/ext/issues"
  },
  "homepage": "https://github.com/DonQuilatte/ext#readme"
}
